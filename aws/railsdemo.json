{
  "containerDefinitions": [
    {
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/railsdemo",
          "awslogs-region": "ap-southeast-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "portMappings": [
        {
          "hostPort": 3000,
          "protocol": "tcp",
          "containerPort": 3000
        }
      ],
      "cpu": 0,
      "environment": [
        {
          "name": "DATABASE_URL",
          "value": "postgres://username:password@host"
        },
        {
          "name": "PORT",
          "value": "80"
        },
        {
          "name": "RACK_ENV",
          "value": "staging"
        },
        {
          "name": "RAILS_ENV",
          "value": "staging"
        },
        {
          "name": "RAILS_LOG_TO_STDOUT",
          "value": "true"
        },
        {
          "name": "RAILS_SERVE_STATIC_FILES",
          "value": "true"
        },
        {
          "name": "SECRET_KEY_BASE",
          "value": "asecret"
        }
      ],
      "image": "924583607971.dkr.ecr.ap-southeast-1.amazonaws.com/demo/rails",
      "name": "rails6demo"
    }
  ],
  "memory": "2048",
  "family": "railsdemo",
  "pidMode": null,
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "networkMode": "awsvpc",
  "cpu": "1024",
  "revision": 5,
  "status": "ACTIVE"
}